<div class="page-header">
  <%= link_to businesses_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_business_path(@business), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show business</h1>
</div>

<dl class="dl-horizontal">
  <dt>Latitude:</dt>
  <dd><%= @business.latitude %></dd>

  <dt>Longitude:</dt>
  <dd><%= @business.longitude %></dd>

  <dt>Name:</dt>
  <dd><%= @business.name %></dd>

  <dt>Address:</dt>
  <dd><%= @business.address %></dd>

</dl>
